<schemalist gettext-domain="gdm-settings">
  <schema id="io.github.realmazharhussain.GdmSettings.window-state"
       path="/io/github/realmazharhussain/GdmSettings/window-state/">

	 <key name="width" type="u">
	   <default>900</default>
	 </key>

	 <key name="height" type="u">
	   <default>500</default>
	 </key>

	 <key name="paned-position" type="u">
	   <default>170</default>
	 </key>

	 <key name="last-visited-page" type="s">
	   <default>"appearance"</default>
	   <choices>
	     <choice value="appearance"/>
	     <choice value="fonts"/>
	     <choice value="display"/>
	     <choice value="sound"/>
	     <choice value="top_bar"/>
	     <choice value="pointing"/>
	     <choice value="misc"/>
	     <choice value="tools"/>
	   </choices>
	 </key>

  </schema>
</schemalist>
