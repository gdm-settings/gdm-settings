msgid "Theme Name"
msgstr "Nombre del tema"

#: data/gschemas/application_id.appearance.gschema.xml.in:20
msgid "Type of Background"
msgstr "Tipo de fondo"

#: data/gschemas/application_id.appearance.gschema.xml.in:21
msgid "Wether GDM background should be an Image, Color or None."
msgstr "Si el fondo de GDM debe ser una imagen, un color o ninguno."

#: data/gschemas/application_id.appearance.gschema.xml.in:33
msgid "Background Image"
msgstr "Imagen de fondo"

#: data/gschemas/application_id.appearance.gschema.xml.in:34
msgid "The image file to use as GDM background"
msgstr "El archivo de imagen que se usará como fondo de GDM"

#: data/gschemas/application_id.appearance.gschema.xml.in:41
